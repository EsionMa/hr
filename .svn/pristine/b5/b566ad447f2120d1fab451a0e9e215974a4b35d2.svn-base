form-item>\r\n\r\n        <!-- 新增下属单位时级别 -->\r\n        <el-form-item v-show=\"title2 == '云平台下属单位信息添加'\" label=\"单位级别\" :label-width=\"fw\" prop='level_id'>\r\n          <el-select @focus=\"selectLevel('addDown')\" v-model=\"form2.unit_level_id\" placeholder=\"请选择单位级别\" style=\"width:90%\">\r\n            <el-option v-for=\"l in level\" readonly :label=\"l.level_name\" :key=\"l.level_id\" :value=\"l.level_id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <!--修改单位时级别 -->\r\n        <el-form-item v-show=\"title2 == '云平台单位信息修改'\" label=\"单位级别\" :label-width=\"fw\" prop='level_id'>\r\n          <el-input :disabled=\"true\" readonly v-model=\"EditlevelName\" auto-complete=\"off\" style=\"width:90%\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"平台显示名称\" :label-width=\"fw\" prop='unit_show_name'>\r\n          <el-input v-model=\"form2.unit_show_name\" auto-complete=\"off\" style=\"width:90%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"单位管理员账号\" :label-width=\"fw\" prop='account'>\r\n          <el-input v-model=\"form2.user_account\" auto-complete=\"off\" style=\"width:90%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"单位管理员密码\" :label-width=\"fw\" prop='user_passwd'>\r\n          <el-input type='password' v-model=\"form2.user_passwd\" auto-complete=\"off\" style=\"width:90%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"单位账号前缀\" :label-width=\"fw\" prop='user_prefix'>\r\n          <el-input v-model=\"form2.user_prefix\" auto-complete=\"off\" style=\"width:90%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"启用状态\" :label-width=\"fw\" prop='unit_status'>\r\n          <el-radio-group v-model=\"form2.unit_status\">\r\n            <el-radio label=\"启用\">启用</el-radio>\r\n            <el-radio label=\"禁用\">禁用</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"visible2 = false;\" size=\"mini\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submit2('form2')\" class=\"yes\" size=\"mini\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\t</div>\r\n</template>\r\n<script>\r\nimport { Loading } from 'element-ui';\r\nimport {mapActions,mapGetters} from 'vuex';\r\nexport default {\r\n  name: 'App',\r\n  data(){\r\n    return {\r\n      visible2:false,\r\n      level:{},\r\n      temp:{},\r\n      form2:{},\r\n      title2:'云平台单位设置',\r\n      fw:'130px',\r\n      li_head://表头：\r\n      `<li class=\"head\">\r\n        <a>\r\n          <div class=\"divTd\" style=\"width:17%\">单位名称</div>\r\n          <div class=\"divTd\" style=\"width:14%\">级别</div>\r\n          <div class=\"divTd\" style=\"width:17%\">平台显示名称</div>\r\n          <div class=\"divTd\" style=\"width:18%\">管理员账号</div>\r\n          <div class=\"divTd\" style=\"width:12%\">单位账号前缀</div>\r\n          <div class=\"divTd\" style=\"width:12%\">状态</div>\r\n          <div class=\"divTd\" style=\"width:10%\">操作</div>\r\n        </a>\r\n      </li>`,\r\n      rules:{//表单非空验证:\r\n        unit_name:[\r\n          {required:true,message:'请输入单位名称',trigger: 'blur'},\r\n        ],\r\n        unit_level_id:[\r\n          {required:true,message:'请选择单位级别',trigger: 'change'},\r\n        ],\r\n        user_account:[\r\n          {required:true,message:'请输入管理员帐号',trigger:'blur'},\r\n        ],\r\n        user_passwd:[\r\n          {required:true,message:'请输入管理员密码',trigger:'blur'},\r\n        ],\r\n        user_prefix:[\r\n          {required:true,message:'请输入正确格式的单位账号前缀(只能以字母开头)',validator: this.CheckTel,trigger:'blur'},\r\n        ],\r\n        unit_status:[\r\n          {required:true,message:'请选择启用状态',trigger: 'change'},\r\n        ],\r\n      }, \r\n      // 新增弹框中的树数据\r\n      defaultProps: {\r\n        children: 'children',\r\n        label: 'name'\r\n      },\r\n      //新增弹框中的树数据\r\n      unitCloudTreeData:[],\r\n      // 上级单位名称\r\n      UnitName:'',\r\n      EditlevelName:'',\r\n    }\r\n  },\r\n  created(){\r\n  \tthis.findAllUnitCloudsAsync().then(()=>{\r\n      \r\n      var $target = $('.unitsClouds tr:has(.fa-folder)');\r\n      console.log($target);\r\n      //$('.unitsClouds tr:has(.fa-folder)')\r\n    });\r\n  },\r\n  computed:{\r\n    ...mapGetters([\r\n    \t'unitCloud',\r\n    \t'unitCloudUpper',\r\n    \t'unitCloudLevel',\r\n    \t'unitCloudTree'\r\n    ]),\r\n  },\r\n  methods:{\r\n    treeColumnClass(level_id){\r\n      return level_id == 2? 'tree_two':((level_id == 3?'tree_three':(level_id == 3?'tree_four':'tree_one')))\r\n    },\r\n    //不能为汉字：\r\n    CheckTel(rule, value, callback){\r\n      if(!value){\r\n          callback(new Error('必须输入用户名'))\r\n      }else{\r\n        var inputValue=new RegExp(\"^[a-zA-Z]*$\", \"\").test(value);\r\n        if (!inputValue) {\r\n        //匹配失败，报错\r\n          callback(new Error('必须以字母开始'));\r\n        }else{\r\n          callback();\r\n        }\r\n      }\r\n    },\r\n    // 删除\r\n    handleDelete(index,row){\r\n      console.log(row);\r\n      let ids = {\r\n        id:row.id,\r\n        level_id:row.level_id,\r\n        prefix:row.user_prefix\r\n      }\r\n      this.delete2(ids);\r\n    },\r\n    delete2(ids){\r\n      this.$confirm('此操作将永久删除该条数据，是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        var loading = Loading.service({ fullscreen: true,text:'正在更改，请稍后...' });\r\n        this.deleteByIdUnitClouds(ids).then((result)=>{\r\n          if(result.data.code == 200){\r\n            loading.close();\r\n            this.$notify({\r\n              title:'成功',\r\n              type: 'success',\r\n              message: '单位信息删除成功'\r\n            });\r\n          }else{\r\n            this.$notify({\r\n              type: 'error',\r\n              message: '单位信息删除失败'\r\n            });\r\n          }\r\n        }).catch((error)=>{\r\n          console.log('单位信息删除失败，后台报错！-->'+error);\r\n        });\r\n      }).catch(() => {});\r\n    },\r\n    // 点击新增\r\n    addUnits(){\r\n      this.handlerunitCloudTree();\r\n      // 清空表单\r\n      this.UnitName = '';\r\n      this.title2 = '云平台单位信息添加';\r\n      this.visible2 = true;\r\n      this.form2 = {unit_status:'启用',unit_upper_id:'0'};\r\n    },\r\n    // 过滤树形结构\r\n    filterNode(value, data) {\r\n      if (!value) return true;\r\n      return data.label.indexOf(value) !== -1;\r\n    },\r\n    //处理树数据\r\n    handlerunitCloudTree(){\r\n      // 一级数据\r\n      let level1 = [];\r\n      this.unitCloud.forEach((item)=>{\r\n        if(item.pid == 0){\r\n          level1.push({\r\n            level_id:item.level_id,\r\n            level_name:item.level_name,\r\n            id:item.id,\r\n            name:item.name,\r\n            children:[]\r\n          })\r\n        }\r\n      })\r\n      // 二级数据\r\n      this.unitCloud.forEach((item)=>{\r\n        level1.forEach((l)=>{\r\n          if(item.pid == l.id){\r\n            l.children.push({\r\n              level_id:item.level_id,\r\n              level_name:item.level_name,\r\n              id:item.id,\r\n              name:item.name,\r\n              pid:item.pid,\r\n              children:[]\r\n            })\r\n          }\r\n        })\r\n      })\r\n      this.unitCloudTreeData = level1;\r\n    },\r\n    // 加载节点\r\n    unitCloudData(node,resolve){\r\n      // 处理一级单位\r\n      if(node.level === 0){\r\n        resolve(this.unitCloudTreeData);\r\n      }\r\n      // 处理二级单位\r\n      if(node.level === 1){\r\n        resolve(node.data.children);\r\n      }\r\n      // 处理三级单位\r\n      if(node.level === 2){\r\n        let arr = [];\r\n        this.unitCloud.forEach((item)=>{\r\n          if(item.pid == node.data.id){\r\n            arr.push(item);\r\n          }\r\n        })\r\n        resolve(arr);\r\n      }\r\n      /*if(node.level ===3){\r\n        let arr = [];\r\n        this.unitCloud.forEach((item)=>{\r\n          if(item.pid == node.data.id){\r\n            arr.push(item);\r\n          }\r\n        })\r\n        resolve(arr);\r\n      }*/\r\n      if(node.level > 2){\r\n        resolve([]);\r\n      }\r\n    },\r\n    // 点击数获取节点\r\n    getunitCloudData(data){\r\n      this.temp = data;\r\n      this.UnitName = data.name;\r\n      this.form2.unit_upper_id = data.id;\r\n      this.level = [];\r\n    },\r\n    // 当前数据改变时树隐藏\r\n    currentChange(node){\r\n      let $unitTree = this.$refs['unitTree'];\r\n      if(node.type == 'type'){\r\n        if (!$unitTree) {\r\n          return false;\r\n        }\r\n      }\r\n      $unitTree.$parent.$parent.hide(2000);    \r\n    },\r\n    // 确认增加\r\n    submit2(formName){\r\n      var loading = Loading.service({ fullscreen: true,text:'正在更改，请稍后...' });\r\n      var flag = 0;\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.saveUnitClouds(this.form2).then((result)=>{\r\n            if(result.data.code == 200){\r\n              loading.close();\r\n              this.$notify({\r\n                title:'成功',\r\n                type: 'success',\r\n                message: '操作成功'\r\n              });\r\n              flag = 0;\r\n            }else if(result.data.code == 300){\r\n              this.$notify({\r\n                type: 'warning',\r\n                message: '用户账号或前缀已存在，请重新输入'\r\n              });\r\n              flag = 1;\r\n            }else{\r\n              console.log('云单位操作失败，后台返回状态码：'+result.data.code);\r\n              flag = 0;\r\n            }\r\n            if(flag == 0){\r\n              this.visible2=false;\r\n            }\r\n          }).catch((error)=>{\r\n            console.log('云单位操作失败，后台报错！-->'+error);\r\n            flag = 0;\r\n            this.visible2=false;\r\n          });\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      }); \r\n    },\r\n    // 根据单位选择单位级别\r\n    selectLevel(data){\r\n      // 新增\r\n      if(data =='add'){\r\n        // 判断上级单位是否为空  如果上级没有选项\r\n        if(this.UnitName == ''){\r\n          this.level = this.unitCloudLevel;\r\n        }else if(this.UnitName != ''){\r\n          // 获取上级单位的下级\r\n          let arr = [];\r\n          this.unitCloudLevel.forEach((item)=>{\r\n            // 当级单位的下面所有级别是可选的级别\r\n            if(this.temp.level_id < item.level_id){\r\n              arr.push(item);\r\n            }\r\n          })\r\n          this.level = arr;\r\n        }\r\n      }\r\n      // 如果是新增下级单位\r\n      else if(data == 'addDown'){}\r\n      // 编辑\r\n      else if(data == 'edit'){}      \r\n    },\r\n    // 新增下级单位\r\n    handleSave(index,row){\r\n      this.title2 = '云平台下属单位信息添加';\r\n      this.visible2 = true;\r\n      // 数据显示\r\n      this.form2 = {unit_status:'启用'};\r\n      this.UnitName = row.name;\r\n      this.form2.unit_upper_id = row.id;\r\n      // 筛选出所有可选的级别\r\n      let downUnit  = []\r\n      // 判断单位级别\r\n      this.unitCloudLevel.forEach((item)=>{\r\n        if(item.level_id > row.level_id){\r\n          downUnit.push(item);\r\n        }\r\n      })\r\n      this.level = downUnit;\r\n    },\r\n    // 修改\r\n    handleEdit(index,row){\r\n      // 判断当前学校是否为学校级别，如果是学校默认不能添加下属机关\r\n      this.visible2 = true;\r\n      this.title2 = '云平台单位信息修改';\r\n      this.form2 = {};\r\n      this.form2 = {\r\n        unit_status:row.unit_status,\r\n        unit_id:row.id,\r\n        unit_name:row.name,\r\n        unit_upper_id:row.pid,\r\n        unit_level_id:row.level_id,\r\n        unit_show_name:row.unit_show_name,\r\n        user_account:row.account,\r\n        user_prefix:row.user_prefix,\r\n        user_passwd:'      '\r\n      }; \r\n      this.EditlevelName = row.level_name;\r\n      // 找出父级的名称   父级单位是不可修改的\r\n      if(row.pid == '0'){\r\n        this.UnitName = '';\r\n      }else{\r\n        this.unitCloud.forEach((item)=>{\r\n          if(item.id == row.pid){\r\n            this.UnitName = item.name;\r\n          }\r\n        })\r\n      }\r\n    },\r\n    // 数据映射\r\n    ...mapActions([\r\n    \t'findAllUnitCloudsAsync',\r\n    \t'findAllUnitClouds',\r\n    \t'saveUnitClouds',\r\n    \t'deleteByIdUnitClouds',\r\n    \t'findUnitClouds'\r\n    ]),\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .unitsClouds .el-table .cell{\r\n    text-align: left;\r\n  }\r\n  .unitsClouds .el-table .el-table__body .fa {\r\n  \tcolor: #448db8;\r\n  }\r\n  .unitsClouds i.options{\r\n  \tpadding: 0 5px;\r\n  \tcursor: pointer;\r\n  }\r\n.unitsClouds .el-button--primary {\r\n    color: #fff;\r\n    background-color: #448db8;\r\n    border-color: #448db8;\r\n}\r\n.tree_two {\r\n  width: 10px;\r\n  display: inline-block;\r\n  text-align: right;\r\n}\r\n.tree_three {\r\n  width: 20px;\r\n  display: inline-block;\r\n  text-align: right;\r\n}\r\n.tree_four {\r\n  width: 30px;\r\n  display: inline-block;\r\n  text-align: right;\r\n}\r\n</style>\r\n<style>\r\n   /*模态框样式*/\r\n  .dialog-title {\r\n    background-color: #448db8;\r\n    height: 52px;\r\n    line-height: 52px;\r\n    font-size: 18px;\r\n    color: #FFF;\r\n\r\n  }\r\n  .el-dialog__headerbtn i.el-dialog__close {\r\n    color: #FFF;\r\n  }\r\n  div.el-dialog__header {\r\n    padding: 0;\r\n  }\r\n  div.el-dialog--center .el-dialog__header {\r\n    padding-top: 0\r\n  }\r\n  .dialog-footer{\r\n    text-align:right;\r\n  }\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/settings/unitsClouds/index.vue","<!-- 学生用户 -->\r\n<template>\r\n\t<div class=\"student\">\r\n    <!-- 搜索栏 -->\r\n\t\t<el-row class='rowTop'>\r\n\t\t\t<el-col :span='16'>\r\n\t\t\t\t<el-form inline :model='stu' size='mini'>\r\n          <el-row>\r\n            <el-col :span='6'>\r\n              <el-form-item>\r\n                <el-select v-model=\"stu.grade_id\" @change='gradeClass(stu.grade_id)' placeholder=\"请选择年级\" >\r\n                  <el-option v-for=\"item in gradeStudent\"  :label=\"item.grade_name\" :value=\"item.grade_id\" :key=\"item.grade_id\">\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span='6'>\r\n              <el-form-item>\r\n                <el-select v-model=\"stu.class_id\" placeholder=\"请选择班级\">\r\n                  <el-option v-for=\"item in classStudent\" :label=\"item.class_name\" :value=\"item.class_id\" :key=\"item.class_id\">\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span='6'>\r\n              <el-form-item>\r\n                <el-input v-model='stu.keys' placeholder='请输入关键字'></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span='6'>\r\n              <el-form-item>\r\n                <el-button type='info' size='small' @click='query' class=\"search\"><!-- <i class=\"fa fa-search\"></i> -->&nbsp;查询</el-button>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n\t\t\t\t</el-form>\r\n\t\t\t</el-col>\r\n\t\t\t<el-col :span='8'>\r\n\t\t\t\t<el-row type='flex' justify='end'>\r\n          <form :action=\"downloadStuUrl\" method='post' style='display:none;'>\r\n            <input type=\"text\" name='grade_id' v-model=\"stu.grade_id\">\r\n            <input type=\"text\" name='class_id' v-model=\"stu.class_id\">\r\n            <input type=\"text\" name='keys' v-model=\"stu.keys\">\r\n            <input type=\"text\" name='dowload_stuInfo' value='1'>\r\n            <input type=\"submit\" id='submitDownload' value=''>\r\n          </form>\r\n          <el-form inline size='mini'>\r\n            <el-col :span='8'>\r\n              <el-form-item class='but'>\r\n                <el-button type='info' size='small' @click='downloadStu' class=\"download\"><!-- <i class=\"fa fa-arrow-circle-down\"></i> -->下载名单</el-button>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span='8'>\r\n              <el-form-item class='but'>\r\n                <el-button type='info' size='small' @click='single3' class=\"add\"><!-- <i class=\"fa fa-plus\"></i> -->新增学生</el-button>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span='8'>\r\n              <el-form-item class='but'>\r\n                <el-button type='info' size='small' @click='batch3' class=\"adds\"><!-- <i class=\"fa fa-plus-circle\"></i> -->批量导入</el-button>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-form>\r\n\t\t\t\t</el-row>\r\n\t\t\t</el-col>\r\n\t\t</el-row>\r\n    <!-- 列表数据 -->\r\n    <el-table :data=\"student\" style=\"width:100%\" class='table' border stripe :header-row-style=\"rowClass\" size='mini' align='center'>\r\n      <el-table-column type=\"index\" label=\"序号\" fixed align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_id\" label=\"id\" fixed v-if='false' align='center'></el-table-column>\r\n      <el-table-column prop=\"class_grade\" label=\"班级\" fixed align='center' width='120'></el-table-column>\r\n      <el-table-column prop=\"stu_account\" label=\"帐号\" fixed align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_name\" label=\"姓名\" fixed align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_number\" label=\"学号\" fixed align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_gender\" label=\"性别\" align='center'></el-table-column>\r\n      <el-table-column prop=\"min_heart\" label=\"最低心率\" align='center'></el-table-column>\r\n      <el-table-column prop=\"avg_heart\" label=\"平均心率\" align='center'></el-table-column>\r\n      <el-table-column prop=\"max_heart\" label=\"最高心率\" align='center'></el-table-column>\r\n      <el-table-column prop=\"first_heart\" label=\"阶段一心率\" width=\"100\" align='center'></el-table-column>\r\n      <el-table-column prop=\"second_heart\" label=\"阶段二心率\" width=\"100\" align='center'></el-table-column>\r\n      <el-table-column prop=\"third_heart\" label=\"阶段三心率\" width=\"100\" align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_hight\" label=\"身高\" align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_weight\" label=\"体重\" align='center'></el-table-column>\r\n      <el-table-column prop=\"stu_status\" label=\"状态\" align='center'></el-table-column>\r\n      <el-table-column label=\"操作\" fixed='right' width=\"80\" align='center'>\r\n        <template slot-scope='scope'>\r\n          <i class=\"options fa fa-edit\" @click=\"edit3(scope.$index, scope.row)\"></i>\r\n\t        <i class=\"options fa fa-trash\" @click=\"delete3(scope.$index, scope.row)\"></i>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <div class=\"block\" :hidden='hideStu'>\r\n      <el-pagination @current-change=\"currentChangeStu\"\r\n        layout=\"prev,pager,next\"\r\n        :total=\"pagesStu\">\r\n      </el-pagination>\r\n    </div>\r\n    \r\n    <!-- 新增学生 -->\r\n    <el-dialog :visible.sync=\"visible3\" center>\r\n      <div slot=\"title\" class=\"dialog-title\">\r\n        {{title3}}\r\n      </div>\r\n      <el-form :model=\"form3\" :rules='rules' class='demo-ruleForm dialog' ref='form3' size='mini'>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"序号\" :label-width=\"fw\" prop='no'>\r\n              <el-input v-model=\"form3.no\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"帐号\" :label-width=\"fw\" prop='stu_account'>\r\n              <el-input v-model=\"form3.stu_account\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"学号\" :label-width=\"fw\" prop='stu_number'>\r\n              <el-input v-model=\"form3.stu_number\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"姓名\" :label-width=\"fw\" prop='stu_name'>\r\n              <el-input v-model=\"form3.stu_name\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"年级\" :label-width=\"fw\" prop='grade_id'>\r\n              <el-select v-model=\"form3.grade_id\" placeholder=\"请选择年级\" style='width:100%;' @change='gradeClass(form3.grade_id)'>\r\n                <el-option v-for='item in gradeStudent' :value='item.grade_id' :key='item.grade_id' :label='item.grade_name'></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"班级\" :label-width=\"fw\" prop='class_id'>\r\n              <el-select v-model=\"form3.class_id\" placeholder=\"请选择班级\" style='width:100%;'>\r\n                <el-option v-for='item in classStudent' :value='item.class_id' :key='item.class_id' :label='item.class_name'></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"手环编号\" :label-width=\"fw\" prop='stu_num'>\r\n              <el-input v-model=\"form3.stu_num\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"平均心率\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.avg_heart\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"最低心率\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.min_heart\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"最高心率\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.max_heart\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"阶段一心率\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.first_heart\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"阶段二心率\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.second_heart\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"阶段三心率\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.third_heart\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"身高\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.stu_hight\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"体重\" :label-width=\"fw\">\r\n              <el-input v-model=\"form3.stu_weight\" auto-complete=\"off\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"性别\" :label-width=\"fw\" prop='stu_gender'>\r\n              <el-radio-group v-model=\"form3.stu_gender\">\r\n                <el-radio label=\"男\">男</el-radio>\r\n                <el-radio label=\"女\">女</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span='12'>\r\n            <el-form-item label=\"启用状态\" :label-width=\"fw\" prop='stu_status'>\r\n              <el-radio-group v-model=\"form3.stu_status\">\r\n                <el-radio label=\"启用\">启用</el-radio>\r\n                <el-radio label=\"禁用\">禁用</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"cancle3\" size='mini'>取 消</el-button>\r\n        <el-button type=\"info\" @click=\"submit3('form3')\" class=\"yes\" size='mini'>确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <!-- 批量导入 -->\r\n    <el-dialog :visible.sync=\"visible3s\" center>\r\n      <div slot=\"title\" class=\"dialog-title\">\r\n        {{title3s}}\r\n      </div>\r\n      <!--\r\n      <el-row>\r\n        <el-col :span=\"18\" :offset=\"4\">\r\n           <el-upload class=\"upload-demo\" ref=\"upload\" center\r\n            action=\"https://jsonplaceholder.typicode.com/posts/\" :show-file-list=\"false\" :on-preview=\"handlePreview\" :on-remove=\"handleRemove\" :file-list=\"fileList\" :auto-upload=\"false\" :on-change=\"handleChange\">\r\n              <el-input style=\"width:420px\" slot=\"trigger\" v-model=\"filename\" size=\"mini\" type=\"info\" placeholder=\"选取文件\">选取文件</el-input>\r\n          </el-upload> \r\n        </el-col>\r\n      </el-row>\r\n      -->\r\n      <el-form :model=\"form3s\" class='dialog3' size='mini'>\r\n        <el-row>\r\n          <el-col :span='6'>\r\n            <el-form-item label=\"\">\r\n              <el-row type='flex' justify='end'>\r\n                <el-button type='info' size='mini' @click='downloadModelStu' class=\"downloadModelStu\">下载模版</el-button>\r\n              </el-row>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span='18'>\r\n            <el-form-item label=\"\">\r\n              <el-input style=\"width:90%\" v-model=\"filename\" placeholder=\"手环批量导入：文件上传\"></el-input>\r\n              <form :action=\"stuUpUrl\" method=\"post\" enctype=\"multipart/form-data\" target='iframe_display' style='position:relative; opacity:0;'>\r\n                <iframe name=\"iframe_display\" style=\"display:none;\"></iframe>   \r\n                <input type=\"file\" id='file' name=\"excelData\" style='position:absolute;top:-30px;left:0; width:90%' value='' @change='changeFileStudent' />\r\n                <input type=\"text\" name='grade_id' v-model=\"stu.grade_id\"/>\r\n                <input type=\"text\" name='class_id' v-model=\"stu.class_id\"/>\r\n                <input type=\"submit\" id='submit' value=\"\" style='position:absolute;top:88px;left:-166px;' />\r\n              </form>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"cancle3s\" size='mini'>取 消</el-button>\r\n        <el-button type=\"info\" @click=\"submit3s\" class=\"adds\" size='mini'>导 入</el-button>\r\n      </div>\r\n    </el-dialog> \r\n\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions,mapGetters} from 'vuex';\r\nimport cg from '../../../store/configure.js';\r\nvar bp = cg.bp; \r\nvar token = sessionStorage.getItem('token');\r\n\r\nexport default {\r\n  name: 'student',\r\n  data(){\r\n    return {\r\n      fw:'100px',\r\n      filename:'',\r\n    \tstu:{},\r\n      title3:'学生用户',\r\n      title3s:'批量导入',\r\n      stuUpUrl:bp+'/manager/student_up?token='+token,\r\n      downloadStuUrl:bp+'/manager/show_student?token='+token,\r\n      form3:{\r\n        no:null,\r\n        stu_account:null,\r\n        stu_number:null,\r\n        stu_name:null,\r\n        grade_id:null,\r\n        class_id:null,\r\n        stu_num:null,\r\n        avg_heart:null,\r\n        min_heart:null,\r\n        max_heart:null,\r\n        first_heart:null,\r\n        second_heart:null,\r\n        third_heart:null,\r\n        stu_hight:null,\r\n        stu_weight:null,\r\n        stu_gender:null,\r\n        stu_status:null\r\n      },\r\n      form3s:{},\r\n      visible3:false,\r\n      visible3s:false,\r\n      fileList:[],\r\n      rules:{\r\n        stu_account:[\r\n          {required:true,message:'请输入帐号',trigger: 'blur'},\r\n        ],\r\n        stu_number:[\r\n          {required:true,message:'请输入学号',trigger: 'blur'},\r\n        ],\r\n        stu_name:[\r\n          {required:true,message:'请输入姓名',trigger: 'blur'},\r\n        ],\r\n        grade_id:[\r\n          {required:true,message:'请选择年级',trigger: 'change'},\r\n        ],\r\n        class_id:[\r\n          {required:true,message:'请选择班级',trigger: 'change'},\r\n        ],\r\n        /*stu_num:[\r\n          {required:true,message:'请输入手环编号',trigger: 'blur'},\r\n        ],*/\r\n        stu_gender:[\r\n          {required:true,message:'请选择性别',trigger: 'change'},\r\n        ],\r\n        stu_status:[\r\n          {required:true,message:'请选择启用状态',trigger: 'change'},\r\n        ],\r\n      }, \r\n    }\r\n  },\r\n  created(){\r\n    this.findAllStu();\r\n  },\r\n  computed:{\r\n    ...mapGetters(['student','gradeStudent','classStudent','pagesStu','hideStu']),\r\n  },\r\n  methods:{\r\n  //分页：\r\n    currentChangeStu(val){\r\n      this.stu.page = val;\r\n      this.findStu(this.stu).then((result)=>{\r\n        \r\n      }).catch((error)=>{\r\n        console.log('学生管理分页操作失败，后台报错'+error);\r\n      });\r\n    },\r\n    gradeClass(g_id){\r\n      this.findClassStudent({grade_id:g_id}).then((result)=>{\r\n      }).catch((error)=>{\r\n        console.log('学生信息查询失败，后台报错'+error);\r\n      });\r\n    },\r\n    downloadModelStu(){//下载模版。\r\n      //跳转到下载地址：\r\n      window.location.href = bp+'/manager/student_download';\r\n    },\r\n    changeFileStudent(){//当用户选择文件时。\r\n      this.filename = $('#file').val();\r\n    },\r\n    downloadStu(){//名单下载。\r\n    //跳转到下载地址：\r\n      $('#submitDownload').trigger('click');//模拟点击隐藏表单。\r\n    //  window.location.href = bp+'/manager/show_student';\r\n    },\r\n    edit3(i,row){\r\n      this.visible3=true\r\n      this.title3='学生信息修改';\r\n      console.log(row);\r\n      this.form3 = row;\r\n      this.form3.no = row.stu_id;\r\n      // this.form3.class_id = row.class_name;\r\n    },\r\n    delete3(i,row){\r\n      this.$confirm('此操作将永久删除该条数据，是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.deleteByIdStu({stu_id:row.stu_id}).then((result)=>{\r\n          if(result.data.code == 200){\r\n            this.$notify({\r\n              title:'成功',\r\n              type: 'success',\r\n              message: '学生信息删除成功',\r\n            });\r\n          }else{\r\n            console.log('学生信息删除失败，后台返回状态码：'+result.data.code);\r\n          }\r\n        }).catch((error)=>{\r\n          console.log('学生信息删除失败，后台报错！-->'+error);\r\n        });\r\n      }).catch(() => {\r\n        this.$notify({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        });          \r\n      });\r\n    },\r\n    single3(){\r\n      this.visible3=true;\r\n      this.title3='单个新增学生';\r\n      this.form3 = {//要清空。\r\n        stu_gender:'女',\r\n        stu_status:'启用',\r\n        no:null,\r\n        stu_account:null,\r\n        stu_number:null,\r\n        stu_name:null,\r\n        grade_id:null,\r\n        class_id:null,\r\n        stu_num:null,\r\n        avg_heart:null,\r\n        min_heart:null,\r\n        max_heart:null,\r\n        first_heart:null,\r\n        second_heart:null,\r\n        third_heart:null,\r\n        stu_hight:null,\r\n        stu_weight:null,\r\n      };\r\n    },\r\n    batch3(){\r\n      if(this.stu.grade_id == undefined || this.stu.class_id == undefined){\r\n        this.$notify({\r\n          type: 'warning',\r\n          message: '请先选择年级和班级！'\r\n        });\r\n      }else{\r\n        this.visible3s=true;\r\n        this.filename='';//清空输入框用户选择的文件。\r\n      }\r\n    },\r\n    submit3(formName){\r\n      var flag=0;\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.saveStu(this.form3).then((result)=>{\r\n            if(result.data.code == 200){\r\n              this.$notify({\r\n                title:'成功',\r\n                type: 'success',\r\n                message: '操作成功'\r\n              });\r\n              flag=0;\r\n            }else if(result.data.code == 300){\r\n              this.$notify({              \r\n                type: 'warning',\r\n                message: '账户已存在,请设置新账号'\r\n              });\r\n              flag=1;\r\n            }else{\r\n              console.log('学生信息操作失败，后台返回状态码：'+result.data.code);\r\n              flag=0;\r\n            }\r\n            if(flag==1){\r\n              this.visible3=true;\r\n            }\r\n          }).catch((error)=>{\r\n            console.log('学生信息操作失败，后台报错！-->'+error);\r\n            flag=0;\r\n          });\r\n          this.visible3=false;\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });  \r\n    },\r\n    submit3s(){\r\n    //  this.$refs.upload.submit();\r\n      $('#submit').trigger('click');\r\n      this.visible3s=false;\r\n    },\r\n    cancle3(){this.visible3=false;},\r\n    cancle3s(){this.visible3s=false;},\r\n    rowClass(row, index) {\r\n      return {\"background-color\":\"#E6EBF5\",'color':'#878D99'}\r\n    },\r\n    query(){\r\n      this.findStu(this.stu).then((result)=>{}).catch((error)=>{\r\n        console.log('学生信息查询失败，后台报错！-->'+error);\r\n      });\r\n    },\r\n    /*handleRemove(file, fileList) {\r\n      console.log(file, fileList);\r\n    },\r\n    handleChange(file, fileList){\r\n      this.filename=file.name;\r\n    },\r\n    handlePreview(file) {\r\n      console.log(file);\r\n    },*/\r\n    ...mapActions(['findAllStu','saveStu','deleteByIdStu','findStu','gradeAndClass','findClassStudent','downLoadStuTable']),\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  *{\r\n    font-size:12px;\r\n  }\r\n  .icon {\r\n\t\tcursor: pointer;\r\n\t\tfont-size: 18px;\r\n\t}\r\n  .table{\r\n   /* margin-top: -3px;*/\r\n    overflow:auto;\r\n  }\r\n  .dialog-footer{\r\n    text-align:right;\r\n  }\r\n  .dialog3 .el-input{\r\n    width:90%;\r\n  }\r\n  .rowTop .el-form .el-form-item{\r\n    margin-top:-3px;\r\n    margin-bottom:11px;\r\n  }\r\n  .rowTop .but{\r\n    margin-right:0;\r\n    margin-left:8px;\r\n  } \r\n  .add{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .search{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .adds{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .yes{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .download{\r\n    font-size:12px;\r\n    border:none;\r\n    background: #448db8;\r\n  }\r\n  .downloadModelStu{\r\n    font-size:12px;\r\n    border:none;\r\n    background: #409EFF;\r\n    border-radius: 0;\r\n    margin-top: 2px;\r\n    margin-left: 3px;\r\n  }\r\n  .student i.options{\r\n  padding: 0 5px;\r\n  cursor: pointer;\r\n  color: #448db8;\r\n}\r\n\r\n</style>\r\n<style>\r\n   /*模态框样式*/\r\n  .dialog-title {\r\n    background-color: #448db8;\r\n    height: 52px;\r\n    line-height: 52px;\r\n    font-size: 18px;\r\n    color: #FFF;\r\n  }\r\n  .el-dialog__headerbtn i.el-dialog__close {\r\n    color: #FFF;\r\n  }\r\n  div.el-dialog__header {\r\n    padding: 0;\r\n  }\r\n  div.el-dialog--center .el-dialog__header {\r\n    padding-top: 0\r\n  }\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/settings/users/student.vue"],"sourceRoot":""}