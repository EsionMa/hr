tton type=\"info\" @click=\"submit2s\" class=\"adds\" size='mini'>导 入</el-button>\r\n      </div>\r\n    </el-dialog> \r\n\r\n    <!-- 用户授权 -->\r\n    <el-dialog :visible.sync=\"visibleAdmin\" center>\r\n      <div slot=\"title\" class=\"dialog-title\">\r\n        {{titleAdmin}}\r\n      </div>\r\n      <el-tree ref=\"auth\" :data=\"userAdminClouds\" node-key=\"module_id\" :default-checked-keys='useradminOne' :props=\"props\"  show-checkbox></el-tree>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n         <el-button @click=\"cancleAdmin\" size='mini'>取 消</el-button>\r\n         <el-button type=\"info\" @click=\"submitAdmin\" class=\"yes\" size='mini'>确 定</el-button>\r\n      </div>\r\n    </el-dialog>  \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions,mapGetters} from 'vuex';\r\nimport cg from '../../../store/configure.js';\r\nvar bp = cg.bp; \r\n\r\nexport default {\r\n  name: 'userClouds',\r\n  data(){\r\n    return {\r\n      fw:'120px',\r\n      tch:{},\r\n      title2:'用户用户',\r\n      title2s:'批量添加用户用户',\r\n      titleAdmin:'用户授权',\r\n      userUpUrl:bp+\"/clouds/user_up\",\r\n      form2:{},\r\n      form2s:{},\r\n      formAdmin:{},\r\n      visible2:false,\r\n      visible2s:false,\r\n      visibleAdmin:false,\r\n      props: {\r\n        label: 'module_name',\r\n        children: 'module_level2'\r\n      },\r\n      filename:'',\r\n      selectTeacher:[],\r\n      rules:{\r\n        user_name:[\r\n          {required:true,message:'请输入姓名',trigger: 'blur'},\r\n        ],\r\n        user_account:[\r\n          {required:true,message:'请输入帐号',trigger: 'blur'},\r\n        ],\r\n        user_passwd:[\r\n          {required:true,message:'请输入密码',trigger: 'blur'},\r\n        ],\r\n        user_tel:[\r\n          {required:true,message:'请输入手机号',trigger: 'blur'},\r\n          {min:11,max:11,message:'长度为11位',trigger: 'blur'},\r\n        ],\r\n        user_gender:[\r\n          {required:true,message:'请选择性别',trigger: 'change'},\r\n        ],\r\n        user_status:[\r\n          {required:true,message:'请选择启用状态',trigger: 'change'},\r\n        ],\r\n      }, \r\n    }\r\n  },\r\n  created(){\r\n    this.findAllUserClouds();\r\n  },\r\n  computed:{\r\n    ...mapGetters(['userClouds','pageTClouds','userAdminClouds','useradminOne']),\r\n  },\r\n  methods:{\r\n    selectArr(val){\r\n      this.selectTeacher = [];\r\n      for(var i=0;i<val.length;i++){\r\n        this.selectTeacher.push(val[i].user_id);\r\n      }     \r\n    },\r\n    //分页\r\n    currentChange(val){\r\n      //以关键字查找，在分页中查找\r\n      this.queryUserClouds({page:val}).then((result)=>{\r\n        \r\n      }).catch((error)=>{\r\n        this.$notify({\r\n          type: 'error',\r\n          message: '操作失败'\r\n        });\r\n      });\r\n    },\r\n    download(){//下载模版。\r\n    //跳转到下载地址：\r\n      window.location.href = bp+'/clouds/user_download';\r\n    },\r\n    changeFileTeacher(){//当用户选择文件时。\r\n      this.filename = $('#file').val();\r\n    },\r\n    edit2(i,row){\r\n      this.visible2=true;\r\n      this.title2='用户信息修改';\r\n      this.form2 = row;\r\n      this.form2.user_passwd = '      ';\r\n    },\r\n    delete2(i,row){\r\n      this.$confirm('此操作将永久删除该条数据，是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.deleteByIdUserClouds({user_ids:row.user_id}).then((result)=>{\r\n          if(result.data.code == 200){\r\n            this.$notify({\r\n              type: 'success',\r\n              message: '用户信息删除成功',\r\n            });\r\n          }else{\r\n            console.log('用户信息删除失败，后台返回状态码：'+result.data.code);\r\n          }\r\n        }).catch((error)=>{\r\n          console.log('用户信息删除失败，后台报错！-->'+error);\r\n        });\r\n      }).catch(() => {\r\n        this.$notify({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        });          \r\n      });\r\n    },\r\n    /*批量删除*/\r\n    batchDelete(){\r\n      var arr1 = this.selectTeacher;\r\n      if(arr1.length < 1){\r\n        this.$notify({\r\n          type: 'warning',\r\n          message: '请先选择用户'\r\n        }); \r\n      }else{\r\n        var userClouds = dd(arr1);\r\n        console.log(userClouds);\r\n         //拼接：\r\n        function dd(a){\r\n          var s = '';\r\n          for(var i=0;i<a.length;i++){\r\n            if(i == a.length-1){\r\n              s += a[i];\r\n            }else{\r\n              s += (a[i]+'@');\r\n            }\r\n          }\r\n          return s;\r\n        }\r\n        this.$confirm('此操作将永久删除该条数据，是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.batchDeleteByIdUserClouds(userClouds).then((result)=>{\r\n            console.log(userClouds);\r\n            if(result.data.code == 200){\r\n              this.$notify({\r\n                type: 'success',\r\n                message: '用户批量信息删除成功',\r\n              });\r\n            }else{\r\n              console.log('用户信息批量删除失败，后台返回状态码：'+result.data.code);\r\n            }\r\n          }).catch((error)=>{\r\n            console.log('用户信息批量删除失败，后台报错！-->'+error);\r\n          });\r\n        }).catch(() => {\r\n          this.$notify({\r\n            type: 'info',\r\n            message: '已取消删除',\r\n          });          \r\n        });\r\n      }\r\n    },\r\n    single2(){\r\n      this.visible2=true;\r\n      this.title2='单个新增用户';\r\n      this.form2={user_gender:'女',user_status:'启用'};\r\n    },\r\n    batch2(){\r\n      this.visible2s=true;\r\n      this.filename='';//清空输入框用户选择的文件。\r\n    },\r\n    // 用户授权\r\n    admin2(){\r\n      if(this.selectTeacher.length == 0){\r\n        this.$notify({\r\n          type: 'warning',\r\n          message: '请先选择用户！'\r\n        });\r\n      }\r\n      else if(this.selectTeacher.length == 1){\r\n        // 获取当前用户的权限  并展示在页面上\r\n        this.visibleAdmin=true;\r\n        this.titleAdmin='用户授权';\r\n        this.adminOne({user_id:this.selectTeacher[0]});  //所有权限展示\r\n        // 查询当前用户权限并选中\r\n      }else if(this.selectTeacher.length > 1){\r\n        this.visibleAdmin=true;\r\n        this.titleAdmin='用户授权';\r\n        this.adminAll();  //所有权限展示\r\n      }\r\n    },\r\n    /*单个新增用户的编辑*/\r\n    submit2(formName){\r\n      var flag = 0;\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.saveUserClouds(this.form2).then((result)=>{\r\n            if(result.data.code == 200){\r\n              this.$notify({\r\n                type: 'success',\r\n                message: '操作成功'\r\n              });\r\n              flag = 0;\r\n            }else if(result.data.code == 300){\r\n              this.$notify({\r\n                type: 'error',\r\n                message: '请求失败，该帐号已经存在，请输入新帐号！',\r\n              });\r\n              flag = 1;\r\n            }else{\r\n              console.log('用户信息操作失败，后台返回状态码：'+result.data.code);\r\n              flag = 0;\r\n            }\r\n            if(flag == 0){\r\n              this.visible2=false;\r\n            }\r\n          }).catch((error)=>{\r\n            console.log('用户信息操作失败，后台报错！-->'+error);\r\n            flag = 0;\r\n            this.visible2=false;\r\n          });\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });  \r\n    },\r\n    submit2s(){\r\n    /*  this.$refs.upload.submit();\r\n      this.saveTchClouds(this.form2).then((result)=>{\r\n        if(result.data.code == 200){\r\n          this.$message({\r\n            type: 'success',\r\n            message: '批量操作成功',\r\n          });\r\n        }else{\r\n          this.$message({\r\n            type: 'error',\r\n            message: '批量操作失败',\r\n          });\r\n        }\r\n      }).catch((error)=>{\r\n        this.$message({\r\n          type: 'error',\r\n          message: '批量操作失败'\r\n        });\r\n      });*/\r\n      $('#submit').trigger('click');\r\n      this.visible2s=false;\r\n      this.$notify({\r\n        type: 'success',\r\n        message: '批量导入成功！'\r\n      });\r\n      // this.findAllTch();//刷新\r\n      //控制路由进行刷新\r\n      var vm = this;\r\n      setTimeout(function(){\r\n        vm.$router.push('/blank/loading');//加载页\r\n        setTimeout(function(){\r\n          vm.$router.push('/settings/usersClouds');\r\n        },100);\r\n      },100);\r\n    },\r\n    submitAdmin(){\r\n      // 获取所有的父级节点id,如果有子节点被选中,就将该父节点push进去数组\r\n      // 获取树形结构的id集合    拿到当前的Id   以及当前选中id的父级id\r\n      let authNodes = this.$refs.auth.getCheckedNodes(true);\r\n      let ch = [];\r\n      let pa = [];\r\n      // 获取子节点id集合\r\n      authNodes.forEach((item)=>{\r\n        ch.push(item.module_id);\r\n        if(item.module_upper_id !=0){\r\n          pa.push(item.module_upper_id);\r\n        }        \r\n      })\r\n      let arr = new Set(pa);\r\n      let arr2 = Array.from(arr).concat(ch);\r\n      // 获取父节点Id集合\r\n      var arr1 = this.selectTeacher;\r\n      var teach = dd(arr1);\r\n      var admin = dd(arr2);\r\n      var idsAdmin = teach + '#' + admin;\r\n      //拼接：\r\n      function dd(a){\r\n        var s = '';\r\n        for(var i=0;i<a.length;i++){\r\n          if(i == a.length-1){\r\n            s += a[i];\r\n          }else{\r\n            s += (a[i]+'@');\r\n          }\r\n        }\r\n        return s;\r\n      }\r\n      this.grantUserClouds(idsAdmin).then((result)=>{\r\n        this.$notify({\r\n          type: 'success',\r\n          message: '用户授权成功',\r\n        });\r\n      }).catch((error)=>{\r\n        console.log('用户授权失败，后台报错！-->'+error);\r\n      });\r\n      this.visibleAdmin=false;\r\n    },\r\n    cancle2(){this.visible2=false;},\r\n    cancle2s(){this.visible2s=false;},\r\n    cancleAdmin(){this.visibleAdmin=false;},\r\n    rowClass(row, index) {\r\n      return {\"background-color\":\"#E6EBF5\",'color':'#878D99'}\r\n    },\r\n    query(){\r\n      this.findUserClouds(this.tch).then((result)=>{}).catch((error)=>{ //查询符合关键字的用户信息\r\n        console.log('用户信息查询失败，后台返回状态码：'+result.data.code);\r\n      });\r\n    },\r\n    ...mapActions(['findAllUserClouds','saveUserClouds','deleteByIdUserClouds','findUserClouds','batchDeleteByIdUserClouds','grantUserClouds','queryUserClouds','adminAll','adminOne']),\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  *{\r\n    font-size:12px;\r\n  }\r\n  .icon {\r\n    cursor: pointer;\r\n    font-size: 18px;\r\n  }\r\n  .table{\r\n   /* overflow:auto; */\r\n    margin-top:5px;\r\n  }\r\n  /* .dialog .el-input{\r\n    width:86%;\r\n  } */\r\n  .dialog-footer{\r\n    text-align:right;\r\n  }\r\n  a{\r\n    display:inline-block;\r\n    width:50px;\r\n  }\r\n  b{\r\n    display:inline-block;\r\n    width:100px;\r\n  }\r\n  .rowTop .el-form .el-form-item{\r\n    margin-top:0px;\r\n    margin-bottom:6px;\r\n  }\r\n  .rowTop .but{\r\n    margin-right:0;\r\n    margin-left:8px;\r\n  }\r\n  .add{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .search{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .adds{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .yes{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .download{\r\n    font-size:12px;\r\n    border:none;\r\n    background: #448db8;\r\n    margin-top: 2px;\r\n    border-radius: 0;\r\n    margin-left: 3px;\r\n  }\r\n  .edit{\r\n    font-size:12px;\r\n    background: #448db8;\r\n    border:none;\r\n  }\r\n  .delete{\r\n    font-size:12px;\r\n    background: #FA5555;\r\n    border:none;\r\n  }\r\n  .userClouds i.options{\r\n    padding: 0 5px;\r\n    cursor: pointer;\r\n    color: #448db8;\r\n  }\r\n</style>\r\n<style>\r\n   /*模态框样式*/\r\n  .dialog-title {\r\n    background-color: #448db8;\r\n    height: 52px;\r\n    line-height: 52px;\r\n    font-size: 18px;\r\n    color: #FFF;\r\n  }\r\n  .el-dialog__headerbtn i.el-dialog__close {\r\n    color: #FFF;